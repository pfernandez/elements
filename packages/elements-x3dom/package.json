{
  "name": "@pfern/elements-x3dom",
  "version": "0.1.11",
  "description": "X3DOM-powered X3D helpers for Elements.js.",
  "author": "Paul Fernandez",
  "license": "MIT",
  "type": "module",
  "types": "./types/x3dom.d.ts",
  "exports": {
    ".": {
      "types": "./types/x3dom.d.ts",
      "import": "./x3dom.js",
      "default": "./x3dom.js"
    }
  },
  "files": [
    "./x3dom.js",
    "./env.d.ts",
    "./src",
    "./vendor",
    "./types"
  ],
  "scripts": {
    "clean:types": "node scripts/clean-types.js",
    "gen:x3d:runtime": "node scripts/generate-x3d.js --runtime",
    "gen:x3d:types": "node scripts/generate-x3d.js --types",
    "fetch:x3dom-src": "node scripts/fetch-x3dom-src.js",
    "build:types": "node scripts/build-types.js",
    "typecheck": "node scripts/typecheck.js",
    "test": "node --test test/*.test.* --test-reporter spec",
    "test:coverage": "node scripts/test-coverage.js"
  },
  "peerDependencies": {
    "@pfern/elements": "*"
  },
  "keywords": [
    "x3d",
    "x3dom",
    "3d",
    "elements"
  ]
}
